<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Record for Class Smart Button Timetable Form -->
    <record id="education_class_division_view_form" model="ir.ui.view">
            <field name="name">education.class.division.view.form.inherit.education.time.table</field>
            <field name="model">education.class.division</field>
            <field name="inherit_id" ref="education_core.education_class_division_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='button_box']" position="inside">
                    <button name="education_time_table.action_education_time_table_view_class_division_wise"
                            type="action" class="oe_stat_button" icon="fa-calendar" string="Timetable"/>
                </xpath>
            </field>
    </record>
</odoo>
