<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Student Admission Sequence -->
        <record id="seq_student_admission" model="ir.sequence">
            <field name="name">Student Admission</field>
            <field name="code">student.admission</field>
            <field name="prefix">ADM</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Student Enrollment Sequence -->
        <record id="seq_student_enrollment" model="ir.sequence">
            <field name="name">Student Enrollment</field>
            <field name="code">student.enrollment</field>
            <field name="prefix">ENR</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Course Sequence -->
        <record id="seq_course" model="ir.sequence">
            <field name="name">Course</field>
            <field name="code">lms.course</field>
            <field name="prefix">CRS</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>
